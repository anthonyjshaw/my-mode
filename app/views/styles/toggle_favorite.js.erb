var styleId = '<%= @style.id%>'
function toggleLikeButton (styleId, innerHTML) {
  const likeButton = document.querySelector(`.like-icon[data-id='${styleId}']`)
  const heart = document.getElementById(`heart-button-${styleId}`)
  console.log(heart);
  likeButton.classList.toggle('red');
  heart.classList.toggle('font-weight-900');
}
toggleLikeButton(styleId)
